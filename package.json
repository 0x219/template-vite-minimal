{
  "name": "template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "turbo vite.build",
    "dev": "turbo vite.dev",
    "lint": "tsc",
    "prepare": "is-ci || husky install",
    "preview": "turbo vite.preview",
    "vite.build": "vite build",
    "vite.dev": "vite --host",
