{
  "name": "template",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install",
    "preview": "wireit"
  },
  "commitlint": {
    "extends": [
      "@0x219"
    ]
  },
  "lint-staged": {
    "*.{cjs,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{js,md,html,css,json}": "prettier --write",
    "!(*pnpm-lock).yaml": "prettier --write"
  },
  "dependencies": {
    "rc-basic": "0.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.0-pre.14",
    "tilg": "0.1.1"
  },
  "devDependencies": {
    "@0x219/commitlint-config": "1.0.2",
    "@commitlint/cli": "17.1.2",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@vitejs/plugin-legacy": "2.1.0",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.7",
    "eslint-plugin-react-hooks": "4.7.0-next-e61fd91f5-20220630",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "prettier-plugin-tailwindcss": "0.1.13",
    "sort-package-json": "1.57.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.2",
    "vite": "3.1.0",
    "vite-plugin-mkcert": "1.9.0",
    "vite-tsconfig-paths": "3.5.0",
    "wireit": "0.7.1"
  },
  "wireit": {
    "dev": {
      "command": "vite",
      "files": [
        "src/**/*.{ts,tsx,svg,css}",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "build": {
      "command": "tsc && vite build",
      "files": [
        "src/**/*.{ts,tsx,svg,css}",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist/**"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "preview": {
      "command": "vite preview",
      "files": [
        "src/**/*.{ts,tsx,svg,css}",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    }
  }
}
